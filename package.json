{
    "name": "gotours",
    "version": "1.0.0",
    "description": "learning nodejs, express, mongodb",
    "main": "app.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "test": "cross-env NODE_ENV=test PORT=5000 mocha --recursive --exit --timeout 30000",
        "dev:build": "cross-env NODE_ENV=production nodemon server.js",
        "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js",
        "build": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js"
    },
    "author": "Tousif",
    "license": "MIT",
    "dependencies": {
        "axios": "^0.27.2",
        "bcrypt": "^5.0.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^16.0.1",
        "express": "^4.17.1",
        "express-mongo-sanitize": "^2.0.2",
        "express-rate-limit": "^6.0.5",
        "helmet": "^5.0.1",
        "hpp": "^0.2.3",
        "html-to-text": "^8.0.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.0.7",
        "morgan": "^1.10.0",
        "multer": "^1.4.2",
        "nodemailer": "^6.4.17",
        "pug": "^3.0.0",
        "sharp": "^0.30.7",
        "slugify": "^1.4.6",
        "stripe": "^11.9.1",
        "validator": "^13.5.2",
        "xss-clean": "^0.1.1"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.19.0",
        "eslint-config-airbnb": "^19.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "mocha": "^10.0.0",
        "mongodb-memory-server": "^8.5.1",
        "nodemon": "^2.0.7",
        "parcel-bundler": "^1.12.4",
        "prettier": "^2.2.1",
        "supertest": "^6.0.1"
